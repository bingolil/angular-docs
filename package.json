{
  "name": "angular-docs",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "test": "jest",
    "test:uncommit": "jest -o",
    "test:band": "jest --runInBand",
    "test:watch": "jest --watch"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.2.3",
    "@angular/cdk": "^19.2.6",
    "@angular/common": "^19.2.3",
    "@angular/compiler": "^19.2.3",
    "@angular/core": "^19.2.3",
    "@angular/forms": "^19.2.3",
    "@angular/platform-browser": "^19.2.3",
    "@angular/platform-browser-dynamic": "^19.2.3",
    "@angular/router": "^19.2.3",
    "@ngx-translate/core": "^16.0.4",
    "@ngx-translate/http-loader": "^16.0.1",
    "date-fns": "^4.1.0",
    "echarts": "^5.6.0",
    "monaco-editor": "^0.40.0",
    "ngx-echarts": "^19.0.0",
    "normalize.css": "^8.0.1",
    "rxjs": "^7.5.2",
    "tslib": "^2.2.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^19.2.4",
    "@angular/cli": "^19.2.4",
    "@angular/compiler-cli": "^19.2.3",
    "@types/jest": "^29.5.0",
    "@types/node": "^22.13.11",
    "jest": "^29.5.0",
    "jest-preset-angular": "^14.5.3",
    "less": "^4.2.0",
    "typescript": "~5.8.2"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/setupJest.ts"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "<rootDir>/src/app/**/*.component.ts",
      "<rootDir>/src/app/**/*-util.ts",
      "<rootDir>/src/app/**/*.directive.ts",
      "<rootDir>/src/app/**/*.pipe.ts",
      "<rootDir>/src/app/**/*.service.ts",
      "<rootDir>/src/app/**/*.interceptor.ts",
      "<rootDir>/src/app/**/*.guard.ts"
    ],
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/src/$1"
    },
    "coverageDirectory": "<rootDir>/coverage",
    "globalSetup": "jest-preset-angular/global-setup"
  }
}
