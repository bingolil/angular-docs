{
  "name": "angular-docs",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "test": "jest",
    "test:uncommit": "jest -o",
    "test:band": "jest --runInBand",
    "test:watch": "jest --watch"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.13",
    "@angular/cdk": "^15.2.2",
    "@angular/common": "^18.2.13",
    "@angular/compiler": "^18.2.13",
    "@angular/core": "^18.2.13",
    "@angular/forms": "^18.2.13",
    "@angular/platform-browser": "^18.2.13",
    "@angular/platform-browser-dynamic": "^18.2.13",
    "@angular/router": "^18.2.13",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "date-fns": "^2.28.0",
    "echarts": "^5.4.1",
    "monaco-editor": "^0.40.0",
    "ngx-echarts": "^15.0.1",
    "normalize.css": "^8.0.1",
    "rxjs": "^7.5.2",
    "tslib": "^2.2.0",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.15",
    "@angular/cli": "^18.2.15",
    "@angular/compiler-cli": "^18.2.13",
    "@types/jest": "^29.5.0",
    "@types/node": "^12.11.1",
    "jest": "^29.5.0",
    "jest-preset-angular": "^13.0.1",
    "typescript": "~5.4.5"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/setupJest.ts"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "<rootDir>/src/app/**/*.component.ts",
      "<rootDir>/src/app/**/*-util.ts",
      "<rootDir>/src/app/**/*.directive.ts",
      "<rootDir>/src/app/**/*.pipe.ts",
      "<rootDir>/src/app/**/*.service.ts",
      "<rootDir>/src/app/**/*.interceptor.ts",
      "<rootDir>/src/app/**/*.guard.ts"
    ],
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/src/$1"
    },
    "coverageDirectory": "<rootDir>/coverage",
    "globalSetup": "jest-preset-angular/global-setup"
  }
}